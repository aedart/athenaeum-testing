{
    "name": "aedart/athenaeum-testing",
    "description": "Testing utilities built on top of Orchestra Testbench and Codeception",
    "keywords": [
        "Athenaeum",
        "Testing"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "codeception/codeception": "4.1.*",
        "codeception/module-asserts": "^1.3",
        "fzaninotto/faker": "1.9.*",
        "mockery/mockery": "1.4.*",
        "orchestra/testbench": "^6.12.0",
        "orchestra/testbench-dusk": "^v6.12.0",
        "aedart/athenaeum-contracts": "^5.11",
        "aedart/athenaeum-container": "^5.11"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Testing\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.11.x-dev"
        }
    }
}
